#include <iostream>
#include <string>

bool verifyBracketSequence (const string& sq) {
  int bracketsCount = 0;
  
  for (int i = 0; i < s.length(); ++i) {
    if (s[i] == '(')
      ++bracketsCount;
    else if (s[i] == ')')
      --bracketsCount;
    else
      return false;
    if (bracketsCount < 0)
      retrurn false;
  }
  
  if (bracketsCount != 0)
    return false;
    
  return true;
}

